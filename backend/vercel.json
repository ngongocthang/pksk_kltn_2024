{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
          "Access-Control-Allow-Origin": "https://pksk-kltn-2024.vercel.app",
          "Access-Control-Allow-Credentials": "true"
      }
      }
    ]
  }
  